{
	"name": "obsidian-fitness",
	"version": "1.0.17",
	"description": "Mobile-first workout tracking with zero-typing set logging, templates, and rest timers.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin"
	],
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@eslint/json": "^0.14.0",
		"@testing-library/dom": "^10.4.1",
		"@types/node": "^20.0.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"jsdom": "^27.3.0",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
